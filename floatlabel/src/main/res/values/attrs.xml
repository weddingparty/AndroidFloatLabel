<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FloatLabelEditText">
        <attr name="maxLength" format="integer" min="0" />
        <attr name="gravity" format="enum">
            <enum name="left" value="3"/>
            <enum name="right" value="5"/>
            <enum name="center" value="17"/>
        </attr>
        <attr name="hint" format="string"/>
        <attr name="inputType" format="enum">
            <enum name="normal" value="0"/>
            <enum name="textPassword" value="1"/>
        </attr>
        <attr name="text" format="string"/>
        <attr name="textSize" format="dimension"/>
        <attr name="textColorHintFocused" format="color"/>
        <attr name="textColorHintUnFocused" format="color"/>
    </declare-styleable>
</resources>